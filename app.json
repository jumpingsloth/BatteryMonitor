{
	"expo": {
		"name": "BatteryMonitor",
		"slug": "battery-monitor",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/BatteryMonitor.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"infoPlist": {
				"UIBackgroundModes": ["location", "fetch"]
			},
			"bundleIdentifier": "dev.jumpingsloth.batterymonitor",
			"buildNumber": "1.0.0"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#FFFFFF"
			},
			"softwareKeyboardLayoutMode": "pan",
			"package": "dev.jumpingsloth.batterymonitor",
			"versionCode": 1
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}
